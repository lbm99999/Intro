<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D5D5D5"
    tools:context="kr.p_e.mprof.www.intro.MainActivity">



    <ImageView
        android:id="@+id/button5"
        android:background="@drawable/logbtn"
        android:layout_width="500dp"
        android:layout_height="150dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="62dp" />

    <EditText
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:id="@+id/id"
        android:hint=" ID"
        android:background="#F6F6F6"
        android:layout_below="@+id/button5"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="90dp" />
    <EditText
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:id="@+id/pw"
        android:hint=" PASSWORD"
        android:inputType="textPassword"
        android:background="#F6F6F6"
        android:layout_marginTop="11dp"
        android:layout_below="@+id/id"
        android:layout_alignLeft="@+id/id"
        android:layout_alignStart="@+id/id" />

    <ImageButton
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:src="@drawable/login"
        android:layout_marginTop="16dp"
        android:layout_below="@+id/pw"
        android:layout_alignLeft="@+id/pw"
        android:layout_alignStart="@+id/pw"
        android:id="@+id/imageButton" />
    <ImageButton
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:src="@drawable/signin"
        android:layout_alignTop="@+id/imageButton"
        android:layout_alignRight="@+id/pw"
        android:layout_alignEnd="@+id/pw" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/jpmz"
        android:layout_alignTop="@+id/imageButton"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="27dp" />



</RelativeLayout>
