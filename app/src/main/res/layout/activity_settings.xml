<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#d3e0c6"
    tools:context="kr.p_e.mprof.www.intro.Settings">

    <Button
        android:id="@+id/button"
        android:layout_width="200dp"
        android:layout_height="100dp"
        tools:layout_editor_absoluteX="44dp "
        tools:layout_editor_absoluteY="126dp"
        android:background="@drawable/jpnbtn"
        android:layout_marginBottom="14dp"
        android:layout_alignParentBottom="true"
        android:layout_alignLeft="@+id/button2"
        android:layout_alignStart="@+id/button2" />/

    <Button
        android:id="@+id/button2"
        android:layout_width="200dp"
        android:background="@drawable/krbtn"
        android:layout_height="100dp"
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="126dp"
        android:layout_marginBottom="44dp"
        android:layout_above="@+id/button3"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/button3"
        android:layout_width="200dp"
        android:background="@drawable/engbtn"
        android:layout_height="100dp"
        tools:layout_editor_absoluteX="239dp"
        tools:layout_editor_absoluteY="126dp"
        android:layout_above="@+id/button"
        android:layout_alignLeft="@+id/button"
        android:layout_alignStart="@+id/button"
        android:layout_marginBottom="42dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="400dp"
        android:background="@drawable/settingimage"
        android:layout_height="80dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />


</RelativeLayout>
